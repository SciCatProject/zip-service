{
  "name": "zip-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run lint",
    "build": "rm -rf ./dist/ && tsc  && cp -r ./src/views ./dist/",
    "win-build": "rmdir /Q /S dist & tsc & Xcopy src\\views dist\\views /E /I ",
    "prestart": "npm run build",
    "start": "node ./dist/index.js",
    "dev": "nodemon ./dist/index.js",
    "lint": "eslint --fix .",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@types/archiver": "^5.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express-fileupload": "^1.1.7",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/morgan": "^1.9.3",
    "@types/rimraf": "^3.0.1",
    "archiver": "^5.3.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "^3.1.6",
    "express": "~4.17.1",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.2",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "node-stream-zip": "^1.14.0",
    "nodemon": "^2.0.11",
    "progressbar.js": "^1.1.0",
    "rimraf": "^2.7.1",
    "sessions": "0.0.2",
    "snyk": "^1.694.0",
    "typescript": "^4.3.5"
  },
  "snyk": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0"
  }
}
