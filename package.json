{
  "name": "zip-service",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "rm -rf ./dist/ && tsc  && cp -r ./src/views ./dist/",
    "win-build": "rmdir /Q /S dist & tsc & Xcopy src\\views dist\\views /E /I ",
    "prestart": "npm run build",
    "start": "node ./dist/index.js",
    "dev": "nodemon ./dist/index.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@types/archiver": "^5.1.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/express-fileupload": "^1.1.5",
    "@types/express-session": "^1.17.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/rimraf": "^3.0.0",
    "archiver": "^5.1.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "ejs": "^2.7.4",
    "express": "~4.17.1",
    "express-fileupload": "^1.2.0",
    "express-session": "^1.17.1",
    "http-errors": "~1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "nodemon": "^2.0.6",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2",
    "node-stream-zip": "^1.12.0",
    "progressbar.js": "^1.1.0",
    "rimraf": "^2.7.1",
    "sessions": "0.0.2",
    "snyk": "^1.434.0"
  },
  "snyk": true
}
